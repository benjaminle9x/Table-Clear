<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.n01216688.testing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity android:name=".ModifyProfile"></activity>
        <activity
            android:name=".checkProfile"
            android:parentActivityName=".CustomerPage" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomePage"
            android:parentActivityName=".CustomerPage" />
        <activity
            android:name=".SettingScreen"
            android:parentActivityName=".CustomerPage" />
        <activity
            android:name=".CustomerPage"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>